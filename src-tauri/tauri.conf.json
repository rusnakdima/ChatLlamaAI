{
  "productName": "chatllamaai",
  "version": "0.5.0",
  "identifier": "com.tcs.chatllamaai",
  "build": {
    "beforeDevCommand": "pnpm start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist/chatllamaai/browser"
  },
  "app": {
    "windows": [
      {
        "title": "Chat Llama AI",
        "fullscreen": false,
        "resizable": true,
        "width": 800,
        "height": 600,
        "minHeight": 400,
        "minWidth": 400
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.ico",
      "icons/128x128.png"
    ],
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    }
  }
}
